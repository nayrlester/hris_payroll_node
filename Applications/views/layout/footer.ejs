    <script>
      $(document).ready(function(){
        $('input[name=daterange]').daterangepicker();
        $.ajax({
           type:"GET",
           url:"/sidebarmenu",
           cache:false,
           success:function(res) {

            for (var i = 0; i < res.length; i++) {
                var link = '';
                if(res[i].link_page == null){
                    link = '<a href="javascript:void(0)" onclick="get_submenu('+res[i].id+')">';
                     $('#main-menu-navigation').append('<li class="nav-item" id="link'+res[i].id+'">'+link+'<i class="'+res[i].icon+'"></i><spandata-i18n="nav.dash.main" class="menu-title">'+res[i].menu+'</span></a><ul class="menu-content" id="submenu_div"></ul></li>');
                }else{
                    link = '<a href="'+res[i].link_page+'">';
                    $('#main-menu-navigation').append('<li class=" nav-item">'+link+'<i class="'+res[i].icon+'"></i><span data-i18n="nav.page_layouts.main" class="menu-title">'+res[i].menu+'</span></a> </li>');
                }
            }

           } 
        });
      });

      function get_submenu(id){

        $("#link"+id).on('click', function() {
            $('.items').last().remove();
        });

        $.ajax({
         type:'GET',
         url:'/submenu/'+id,
         cache:false,
         success:function(res) {

            for (var i = 0; i < res.length; i++) {
                $("#link"+id+">#submenu_div").append('<li class="active items"><a href="'+res[i].link_page+'" data-i18n="nav.dash.main" class="menu-item">'+res[i].sub_menu+'</a></li>');
            }
         }
        });
      }

      
    </script>