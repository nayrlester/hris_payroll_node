

    <div class="app-content content container-fluid">
        <div class="content-wrapper">
          <div class="content-header row">
          </div>
          <div class="content-body">
              <div class="row">
                  <div class="col-xs-12">
                      <div class="card">
                          <div class="card-header">
                              <h1 class="card-title">Salary Section</h1>
                              <a class="heading-elements-toggle"><i class="icon-ellipsis font-medium-3"></i></a>
                              <div class="heading-elements">
                                  <ul class="list-inline mb-0">
                                      <li><a data-action="collapse"><i class="icon-minus4"></i></a></li>
                                      <li><a data-action="reload"><i class="icon-reload"></i></a></li>
                                      <li><a data-action="expand"><i class="icon-expand2"></i></a></li>
                                  </ul>
                              </div>
                          </div>
                          <div class="card-body collapse in">
                              <div class="row">
                                  <div class="col-lg-12 col-xl-12">
                                        <div class="col-md-6">
                                          <div class="card-block">
                                              <h4 class="card-title">Select Date Range</h4>
                                              <fieldset class="form-group position-relative has-icon-left mb-1">
                                                <input type="text" name="daterange" id="daterange" class="form-control" value = "<%- daterange %>" required /> 
                                                <div class="form-control-position">
                                                      <i class="icon-department"></i>
                                                  </div>
                                              </fieldset>
                                          </div>
                                        </div>
                                        <div class="col-md-6">
                                          <div class="card-block">
                                              <h4 class="card-title">Select Client</h4>
                                              <fieldset class="form-group position-relative has-icon-left mb-1">
                                                  <select class = "form-control" id="pclient" required>
                                                      <option value = "">-- Select Client Name --</option>
                                                      <% client.forEach(function(rows){ %>
                                                        <option value = "<%- rows.id %>"><%- rows.client_name %></option>
                                                      <% }) %>
                                                  </select>  
                                                  <div class="form-control-position">
                                                      <i class="icon-department"></i>
                                                  </div>
                                              </fieldset>
                                          </div>
                                        </div>
                                          <div class="card-block">
                                              <div class="table-responsive">
                                                  <div class="col-sm-12">
                                                      <h3>Select Employee</h3>
                                                      <table class="table" id = "pick-client" cellspacing="0" width="100%">
                                                          <thead class="bg-info">
                                                              <tr>
                                                                  <th>Full Name</th>
                                                                  <th>Rate</th>
                                                                  <th>Position</th>
                                                                  <th>Net</th>
                                                                  <th>Action</th>
                                                              </tr>
                                                          </thead>
                                                          <tbody></tbody>
                                                      </table>
                                                  </div> 
                                              
                                              </div>
                                          </div>
                                          <div class="card-block">
                                              <div class="table-responsive">
                                                  <div class="col-sm-12">
                                                      <button class = "btn btn-primary" id = "process">Process Payroll</button>
                                                      
                                                  </div> 
                                              
                                              </div>
                                          </div>
                                  </div>
                              </div>
                          </div>
                      </div>
                  </div>
                  </div>
              </div>
          <div class="content-body">
            <div class="row">
                <div class="col-xs-12">
                    <div class="card">
