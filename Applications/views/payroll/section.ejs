                                          <table class="table" cellspacing="0" width="100%">
                                              <thead class="bg-info">
                                                  <tr>
                                                  <th></th>
                                                  <th>Amount</th>
                                                  </tr>
                                              </thead>
                                              <tbody>
                                                  <tr>
                                                  <td>COLA</td>
