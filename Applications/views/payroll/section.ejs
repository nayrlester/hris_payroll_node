                                                      <tr>
                                                          <td><%- row.description %></td>
                                                          <td><input type="text" name="" id="loan-type-<%- row.id %>" code= "<%- row.id %>" class="form-control loan-get" readonly></td>
                                                        </tr>
                                                  <% }); %>
                                                  </thead>
                                                </table>
                                          

                                                <h3>Total Earnings</h3>
                                                <table class="table" cellspacing="0" width="100%">
                                                      <tr>
                                                          <th>Total Deductions =</th>
                                                          <td><input type="text" name="total_deductions" id="total_deductions" value="0.00" class="form-control reset" readonly></td>
                                                      </tr>
